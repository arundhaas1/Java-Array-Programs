      //print the duplicate elements of an array
      
      [1,2,3,4,2,4,3] => duplicate are 2,3,4 .
      
package com.dhas;
import java.util.Arrays;
import java.util.Scanner;
public  class Main {
    public static void main(String[] args) {
        int [] arr ={1, 2, 3, 2, 5,1};    //create an array

        for (int i=0;i<arr.length;i++){  //this loop for check every digit of array
            for (int j=i+1;j< arr.length;j++){   this loop to check and remove the previous checked data by i+1.
            if(arr[i]==arr[j]){  //if both value gets equal value is printed
                   System.out.println(arr[i]);

               }
            }
        }
    }
}
